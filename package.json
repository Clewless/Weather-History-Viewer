{
  "name": "weather-history-viewer",
  "version": "1.0.0",
  "description": "A web application for viewing historical weather data.",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node dist/server/server.js",
    "start:all": "concurrently \"npm start\" \"npm run start:server\"",
    "prod": "npm run build && npm run build:server && npm run start:server",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "weather",
    "history",
    "open-meteo"
  ],
  "author": "Clay Clewless <clewlessclay@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/tz-lookup": "^6.1.2",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^9.36.0",
    "eslint-config-preact": "^2.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "html-webpack-plugin": "^5.6.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "preact": "^10.27.2",
    "preact-render-to-string": "^6.6.1",
    "tz-lookup": "^6.1.25",
    "validator": "^13.15.15"
  }
}
