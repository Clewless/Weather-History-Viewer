{
  "name": "weather-history-viewer",
  "version": "1.0.0",
  "description": "A web application for viewing historical weather data.",
  "scripts": {
    "start": "webpack-dev-server --open --port 8081",
    "build": "webpack",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node dist/server.js",
    "start:all": "concurrently \"npm start\" \"npm run start:server\"",
    "prod": "npm run build && npm run build:server && npm run start:server",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "weather",
    "history",
    "open-meteo"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
